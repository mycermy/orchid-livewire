{
  "name": "alexsabur/orchid-livewire",
  "description": "A Livewire macro for Orchid Platform",
  "license": "MIT",
  "keywords": [
    "orchid",
    "laravel",
    "livewire"
  ],
  "type": "library",
  "authors": [
    {
      "name": "Alex Sabur",
      "email": "alexsabur@live.ru"
    }
  ],
  "require": {
    "orchid/platform": "^14.0",
    "livewire/livewire": "^3.0"
  },
  "require-dev": {
    "orchestra/testbench": "^7.0|^8.0",
    "phpunit/phpunit": "^11.0"
  },
  "autoload": {
    "psr-4": {
      "AlexSabur\\OrchidLivewire\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests"
    }
  },
  "scripts": {
    "phpunit": "phpunit"
  },
  "extra": {
    "laravel": {
      "providers": [
        "AlexSabur\\OrchidLivewire\\ServiceProvider"
      ]
    }
  },
  "config": {
    "preferred-install": "dist",
    "sort-packages": true,
    "optimize-autoloader": true
  }
}
